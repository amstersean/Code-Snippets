@keyframes growIn {
  0% {
    transform: scale(0);
    opacity: 0; }
  100% {
    transform: scale(1);
    opacity: 1; } }
@keyframes slideIn {
  0% {
    transform: translateX(50%);
    opacity: 0; }
  100% {
    transform: translateX(0px);
    opacity: 1; } }
@keyframes blip {
  0% {
    opacity: 0;
    transform: scale(0);
    border: 5px solid #a4a9ad; }
  50% {
    opacity: 1; }
  100% {
    opacity: 0;
    border: 1px solid #a4a9ad;
    transform: scale(1.25); } }
